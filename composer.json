{
    "name": "nanopkg/laravel-bulk-sms-bd",
    "description": "With this package you can easily integrate bulk sms system in your project with bulksmsbd.com API of Bangladesh.",
    "keywords": [
        "nanopkg",
        "laravel",
        "laravel-bulk-sms-bd",
        "package",
        "library",
        "Plugin",
        "bulksmsbd",
        "laravel-package",
        "sms-api",
        "laravel-plugin",
        "bulksms",
        "laravel-bulksms",
        "laravel-bulksmsbd",
        "laravel-sms-gateway"
    ],
    "homepage": "https://github.com/nanopkg/laravel-bulk-sms-bd",
    "license": "MIT",
    "authors": [
        {
            "name": "IQBAL HASAN",
            "email": "iqbalhasan.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Nanopkg\\LaravelBulkSmsBd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nanopkg\\LaravelBulkSmsBd\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nanopkg\\LaravelBulkSmsBd\\BulkSmsBdServiceProvider"
            ],
            "aliases": {
                "LaravelBulkSmsBd": "Nanopkg\\LaravelBulkSmsBd\\Facades\\BulkSmsBd"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
